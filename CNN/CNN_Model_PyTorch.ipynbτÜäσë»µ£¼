    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[885   1  24  19   1   1  66   0   3   0]\n",
      " [  5 975   0  14   2   0   1   0   3   0]\n",
      " [ 15   0 860  10  61   0  51   0   3   0]\n",
